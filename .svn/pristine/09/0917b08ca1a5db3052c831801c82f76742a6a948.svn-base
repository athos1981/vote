/* 树状图相关CSS */
.base-frame{
  position:relative;
}

.ztree{
  opacity:0;
  position:absolute;
  left:0;
  top:26px;
  background:#fff;
  margin:0 0;
  transition: all .3s ease;
  -moz-transition: all .3s ease;
  -webkit-transition: all .3s ease;
  -o-transition: all .3s ease;
  -ms-transition: all .3s ease;
  height:0;
  overflow-y:auto;
  overflow-x:visible;
}

.color-dark:hover .ztree{
  opacity:1;
  z-index:100;
  height:330px;
}

