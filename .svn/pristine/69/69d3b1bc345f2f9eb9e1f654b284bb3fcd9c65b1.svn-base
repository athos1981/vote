                minH = Math.min(bodyH, htmlH),
                h = demoIframe.height() >= maxH ? minH : maxH;
            if (h < 530) h = 530;
            demoIframe.height(h);
        }

        // 基于准备好的dom，初始化echarts实例
        var myChart = echarts.init(document.getElementById('userstory'));

        // 指定图表的配置项和数据
        var option = {
            title: {
                text: '用户故事走势图'
            },
            tooltip: {
                trigger: 'axis'
            },
            legend: {
                data: ['排期后用户故事1', '排期后用户故事2']
            },
            grid: {
                left: '3%',
                right: '4%',
                bottom: '3%',
                containLabel: true
            },
            toolbox: {
                feature: {
                    saveAsImage: {}
                }
            },
            xAxis: {
                type: 'category',
                boundaryGap: false,
                data: ['3-1', '3-2', '3-3', '3-4', '3-5', '3-6', '3-7']
            },
            yAxis: {
                type: 'value'
            },
            series: [{
                name: '排期后用户故事1',
                type: 'line',
                stack: '总量',
                itemStyle: {
                    normal: {
                        color: "#00c0ef" //图标颜色
                    }
                },
                data: [120, 132, 101, 134, 90, 230, 210]

            }, {
                name: '排期后用户故事2',
                type: 'line',
                stack: '总量',
                itemStyle: {
                    normal: {
                        color: "#dd4b39" //图标颜色
                    }
                },
                data: [220, 182, 191, 234, 290, 330, 310]
            }]
        };

        // 使用刚指定的配置项和数据显示图表。
        myChart.setOption(option);

    });
    </script>
</body>

